{
  "extends": "@efox/emp-tsconfig",
  "compilerOptions": {
    "jsx": "react",
    "noImplicitAny": false,
    "baseUrl": ".",
    "plugins": [
      {
        "name": "typescript-plugin-css-modules"
      }
    ],
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*", "@types/**/*"]
}
